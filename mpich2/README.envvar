    Default: ""

MPIR_CVAR_NEMESIS_SHM_EAGER_MAX_SZ
    Aliases: MPIR_PARAM_NEMESIS_SHM_EAGER_MAX_SZ
             MPICH_NEMESIS_SHM_EAGER_MAX_SZ
