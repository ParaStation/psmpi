apply: 
	$(ACTION) $(ACTION_INPUT)
	(cd src && ${MAKE} apply )
	(cd netmod && ${MAKE} apply )
	(cd utils && ${MAKE} apply )
clean: 
	-@rm -f *.o ${srcdir}/*.o 
	-@rm -f *.lo ${srcdir}/*.lo
	-@rm -f ${srcdir}/*.bb ${srcdir}/*.bbg ${srcdir}/*.da
	-@rm -f ${srcdir}/*.gcda ${srcdir}/*.gcno
	-@rm -f *.gcov *.bb *.bbg *.da *.gcda *.gcno
	(cd src && ${MAKE} clean )
	(cd netmod && ${MAKE} clean )
	(cd utils && ${MAKE} clean )
distclean:  clean distclean-xxx remove-makefile
distclean-xxx:
	if [ -s src/Makefile ] ; then (cd src && ${MAKE} distclean ) ; fi
	if [ -s netmod/Makefile ] ; then (cd netmod && ${MAKE} distclean ) ; fi
	if [ -s utils/Makefile ] ; then (cd utils && ${MAKE} distclean ) ; fi
	-@rm -f Makefile
	-@rm -rf autom4te*.cache
	-@rm -rf $(DEPS_DIR)
	-@rm -f TAGS
maintainer-clean:  distclean-xxx remove-genmakefiles
	(cd src && ${MAKE} maintainer-clean )
	(cd netmod && ${MAKE} maintainer-clean )
	(cd utils && ${MAKE} maintainer-clean )
remove-makefile:
	rm -f Makefile
remove-genmakefiles:
	rm -f  Makefile.in Makefile
all-redirect:
	@(cd src && ${MAKE} all )
	@(cd netmod && ${MAKE} all )
	@(cd utils && ${MAKE} all )

coverage: 
	(cd src && ${MAKE} coverage )
	(cd netmod && ${MAKE} coverage )
	(cd utils && ${MAKE} coverage )
