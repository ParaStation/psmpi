.SUFFIXES:
.SUFFIXES: .o .cxx .txt 
.cxx.o:
	@if [ "x$(VERBOSE)" != "x1" ] ; then \
	  echo "  CXX             $<" ; \
	else \
	  echo $(CXX_COMPILE) -c $< ; \
	fi
	@$(CXX_COMPILE) -c $<
.cxx.txt:
	$(CXXCPP) $(INCLUDES) $(CPPFLAGS) $< >$*.txt
.cxx:
	$(CXX_COMPILE) -o $* $< $(LDFLAGS) $(LIBS)

